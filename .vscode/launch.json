{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch C++ Program",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/test_task",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "sourceFileMap": {
        "/home/dev/test_task": "${workspaceFolder}"
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "CMake: Build"
    }
  ]
}